{
  "name": "vs-copland",
  "displayName": "vs-copland",
  "description": "Vs code extension for copland syntax",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "copland",
      "aliases": ["Copland", "copland"],
      "extensions": [".cop"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "copland",
      "scopeName": "source.cop",
      "path": "./syntaxes/copland.tmLanguage.json"
    }]
  }
}
