{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Copland",
	"patterns": [
		{
			"include": "#inital_place"
		},
		{
			"include": "#place"
		},
		{
			"include": "#phrase"
		},
		{
			"include": "#symbol"
		},
		{
			"include": "#branch"
		},
		{
			"include": "#comment"
		}
	],
	"repository": {
		"inital_place": {
			"patterns": [{
				"name": "inital_place.???.copland",
				"match": "/\\*[a-zA-Z]*[0-9]*:/gm"
			}]
		},
		"place": {
			"patterns": [{
					"name": "place.????.copland",
					"match": "/\\b[a-zA-Z]*[0-9]*/gm"
				}]
		},
		"phrase":{
			"patterns": [{
				"name": "phrase.???.copland",
				"match": ""
			}]
		},
		"symbol":{
			"patterns": [{
				"name": "symbol.????.copland",
				"match": ""
			}]
		}
	},
	"scopeName": "source.cop"
}